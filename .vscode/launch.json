{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run tests",
      "program": "${workspaceRoot}/node_modules/jest/bin/jest",
      "args": ["-i"],
      "runtimeArgs": ["--no-lazy"],
      "cwd": "${workspaceRoot}",
      "env": {
        "NODE_ENV": "development"
      },
      "sourceMaps": true
    }
  ]
}
